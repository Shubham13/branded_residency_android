<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="NetworkImageView">
        <attr name="networkUrl" format="string" />
        <attr name="cacheEnable" format="boolean" />
        <attr name="defaultImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="DVButton">
        <attr name="dv_backgroundColor" format="color" />
        <attr name="dv_radius" format="integer" />
        <attr name="dv_strokeColor" format="color" />
        <attr name="dv_strokeWidth" format="integer" />
        <attr name="dv_strokeDashWidth" format="integer" />
        <attr name="dv_strokeDashGap" format="integer" />
    </declare-styleable>

    <declare-styleable name="DVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension" />
        <attr name="indicatorName" format="string" />
        <attr name="indicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SingleRowCalendar">
        <attr name="pastDaysCount" format="integer" />
        <attr name="futureDaysCount" format="integer" />
        <attr name="includeCurrentDate" format="boolean" />
        <attr name="initialPositionIndex" format="integer" />
        <attr name="multiSelection" format="boolean" />
        <attr name="deselection" format="boolean" />
        <attr name="longPress" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Toggle">
        <attr name="textOn" format="string" />
        <attr name="textOff" format="string" />
        <attr name="colorOff" format="color" />
        <attr name="colorOn" format="color" />
        <attr name="colorBorder" format="color" />
        <attr name="colorDisabled" format="color" />
        <attr name="on" format="boolean" />
        <attr name="android:textSize" />
        <attr name="android:enabled" />
    </declare-styleable>

    <declare-styleable name="Calender">
        <attr name="calender_background" format="color" />
        <attr name="selector_color" format="color" />
        <attr name="selected_day_text_color" format="color" />
        <attr name="current_month_day_color" format="color" />
        <attr name="off_month_day_color" format="color" />
        <attr name="month_color" format="color" />
        <attr name="week_name_color" format="color" />
        <attr name="next_icon" format="reference" />
        <attr name="previous_icon" format="reference" />
        <attr name="is_show_header" format="boolean" />
    </declare-styleable>

    <declare-styleable name="OtpViewTheme">
        <attr format="reference" name="otpViewStyle"/>
    </declare-styleable>

    <declare-styleable name="OtpView">
        <attr format="integer" name="OtpItemCount"/>
        <attr format="dimension" name="OtpItemWidth"/>
        <attr format="dimension" name="OtpItemHeight"/>
        <attr format="dimension" name="OtpItemRadius"/>
        <attr format="dimension" name="OtpItemSpacing"/>
        <attr format="dimension" name="OtpLineWidth"/>
        <attr format="reference|color" name="OtpLineColor"/>
        <attr format="enum" name="OtpViewType">
            <enum name="rectangle" value="0"/>
            <enum name="line" value="1"/>
            <enum name="none" value="2"/>
        </attr>
        <attr name="android:cursorVisible"/>
        <attr name="android:textAllCaps"/>
        <attr format="dimension" name="OtpCursorWidth"/>
        <attr format="reference|color" name="OtpCursorColor"/>
        <attr name="android:itemBackground"/>
        <attr format="boolean" name="OtpHideLineWhenFilled"/>
        <attr format="boolean" name="OtpState_filled"/>
        <attr format="boolean" name="OtpRtlTextDirection"/>
        <attr format="string" name="OtpMaskingChar"/>
    </declare-styleable>

</resources>