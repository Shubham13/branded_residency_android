<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
            name="monthlyVM"
            type="com.digivalet.brandresidential.ui.fragments.sheetfragments.MonthlyFragment" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.digivalet.brandresidential.ui.views.calender.Calender
            android:layout_marginTop="@dimen/_20sdp"
            android:id="@+id/calender_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_marginEnd="@dimen/_15sdp"
            app:is_show_header="false"
            app:off_month_day_color="@android:color/darker_gray" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>